{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@frontity/now"
      }
    ],
    "redirects": [ 
      { "source": "/about", "destination": "/board" },
      { "source": "/([^\/]+)\/([0-9]{4})\/([0-9]{2})\/([0-9]{2})\/([^\/]+)/", "destination": "https://africabusiness.news/$2/$3/$4/$5" }
    ]
  }

